  "devDependencies": {
    "@stryker-mutator/core": "^4.5.1",
    "@stryker-mutator/jest-runner": "^4.5.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "gh-pages": "^3.2.3",
    "jest": "^26.6.3",
